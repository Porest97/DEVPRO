@model IEnumerable<PROContacts.Models.Season>

@{
    ViewData["Title"] = "Index";
}

<h1>Seasons</h1>

<p>
    <a class="btn btn-success" asp-area="" asp-controller="Seasons" asp-action="Create">Create New Season</a>
    <a class="btn btn-danger" asp-area="" asp-controller="Home" asp-action="People">Back to People</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SeasonName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SeasonName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
